<table id="admin-form" class="table table-bordered">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Prénom</th>
      <th scope="col">Nom</th>
      <th scope="col">Email</th>
      <th scope="col">Téléphone</th>
      <th scope="col">Artiste?</th>
      <th scope="col">TODO</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr> 
      <td><%= user.id %></td>
      <td><%= user.first_name %></td>
      <td><%= user.last_name %></td>
      <td><%= user.email %></td>
      <td><%= user.phone %></td>
      <td><%= user.is_artist %></td>
      <% if user.is_artist == false %>
        <td> <%= link_to "Créer artiste", dashboard_update_path(user.id), method: :put, class: "btn btn-danger" %></td>
      <% else %>
      <% end %>
    </tr>
    <% end %>
  </tbody>
</table>

